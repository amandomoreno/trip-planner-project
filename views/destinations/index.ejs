<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Popular Destinations</th>
      <th>Special Offers</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% destinations.forEach(function(d) { %>
      <tr>
        <td><%= d.title %></td>
        <td><%= d.releaseYear %></td>
        <td><a href="/movies/<%= d._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>